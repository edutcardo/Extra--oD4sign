{
  "builds": [
    {
      "src": "/public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}